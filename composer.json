{
    "name": "batyukovstudio/laravel-notification-channels-pushsms-ru",
    "description": "PushSMS notifications channel for Laravel",
    "keywords": ["laravel", "notifications", "pushsms"],
    "homepage": "https://github.com/batyukovstudio/laravel-notification-channels-pushsms-ru",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "batyukovstudio",
            "role": "studio",
            "email": "info@batyukovstudio.com",
            "homepage": "https://github.com/batyukovstudio"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/queue": "5.1 - 5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/notifications": "5.1 - 5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "5.1 - 5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PushSMS\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\PushSMS\\PushSmsServiceProvider"
            ]
        }
    }
}
